<div
  {{update-document-title this.count}}
  {{on-tick (action this.incrementSeconds)}}
>
  Passed Prop: {{@someProp}}<br>
  Elapsed Seconds: {{this.seconds}}<br>
  Elapsed Minutes: {{this.minutes}}<br><br>

  <button {{action this.bumpTitleCount}}>
    Bump document title count
  </button><br><br>

  <button {{action this.clearElapsedTime}}>
    Clear Elapsed Time
  </button>
</div>